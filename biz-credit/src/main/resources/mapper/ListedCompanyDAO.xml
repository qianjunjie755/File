<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.biz.credit.dao.ListedCompanyDAO">
    <select id="queryStockCode" resultType="java.lang.String">
        SELECT stock_code FROM t_listed_company where company_name = #{companyName} and status = 1
    </select>
</mapper>